{
  "name": "ai-diet-coach-functions",
  "description": "Firebase Cloud Functions for AI Diet Coach",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "openai": "^4.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "typescript": "^4.9.0",
    "@types/cors": "^2.8.13"
  },
  "private": true
}